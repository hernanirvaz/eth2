#!/bin/bash

meeu=$(whoami)
cunm=$(uname -n)
cust=$(uname -n|cut -c1-3)
zip1='192.168.8.25'
zip2='192.168.8.28'
zip3='192.168.8.31'
zip4='192.168.8.34'
zipg=$(ip r|grep default|cut -d' ' -f3)
znm1='hrv-zotac1'
znm2='hrv-zotac2'
znm3='hrv-zotac3'
znm4='hrv-zotac4'
vip1='161.97.85.32'
vip2='161.97.85.32'
vip3='161.97.85.32'
vnm1='vmi1007895.contaboserver.net'
vnm2='vmi1007895.contaboserver.net'
vnm3='vmi1007895.contaboserver.net'
if [[ $cust = hrv ]];then ciec=$zip1;else ciec=$vip1;fi;[[ $cunm = $znm4 ]] && ciec=$zip4
if [[ $cust = hrv ]];then cicc=$zip3;else cicc=$vip1;fi;[[ $cunm = $znm4 ]] && cicc=$zip4
cuvc=$znm4
nxvc=$znm4

p00='/usr/local/bin'
p01='/var/log/syslog'
p02='/home/eth'
p03='/dev/null'
p04='/etc/systemd/system'
p05='/var/lib/lighthouse';[[ $cunm = $znm2 ]] && p05='/sata/lighthouse';[[ $cunm = $znm4 ]] && p05='/sata/lighthouse'
p06='/sata/eth';[[ $cunm = $vnm1 ]] && p06=$p02
p07='/usr/bin'
p08='/var/lib/goethereum'
p09='/var/lib/javaethereum'
