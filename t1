s20='named'
s21='plexmediaserver'
s22='nginx'
s23="JAVA_HOME='/usr/lib/jvm/java-11-openjdk-amd64'"
s24=""
s24="$s24\n1 [$(snc $cuvc)]\tt tvc (vp,vd,cp slash -> $(snc $nxvc))"
s24="$s24\n2\t\tsleep 7m"
s24="$s24\n3 [$(snc $nxvc)]\trm lock;t3"
s24="$s24\n4\t\tcheck slash,srv,dirs"
s24="$s24\n5\t\trm lock;t vea\n"
s25='0x27C7f54e48956A906aF2CBFBC8684b437776403d'
s26='0x534029B6371dc4453DD750Bc1198181F55C859fe'
s27="--connection=http://$zip4:5052 --allow-insecure-connections"
s28='Content-Type: application/json'
s29='offline-preparation'
s30='change-operations'
s31=1606824023
s32="http://$cicc:5052/eth/v1/validator/duties"
s33='https://boost-relay.flashbots.net/relay/v1/data/validator_registration?pubkey'
s34='.data[].slot'
s35='.data[].validator_index'
s36=3

