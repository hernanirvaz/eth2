e37="$e34;$e35;$e34;$e35;$e34;$e35;$e36"                                                           # FINAL
e38='s%(hash|root) *= *([^ ,]*),? *%%g;s%#033\[[0-9;]+m%%g'                                        # EL
e39='s%(finalized_root|root|block|head|parent_hash|pubkey|block_hash|peer_id) *: *([^ ,]*),? *%%g' # CL
e40='s%(Some|Slot) *\(([^ ]*)\)%\2%g;s%,? *(module) *: *([^ ,]*),? *%%g'                           # CL
