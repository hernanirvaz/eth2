p29='/etc/bind/named.conf.options'
p30='/etc/default/named'
p31='/etc/ntp.conf'
p32='/etc/nginx/conf.d/plexmediaserver.conf'
p33='/etc/transmission-daemon/settings.json'
p34='/etc/hosts'
p35='/etc/ssh/ssh_config.d/ztc.conf'
p36='/etc/environment'
p37='/etc/fstab'
p38='/etc/prometheus'
p39="$p02/system.restart"
p40="$p02/geth.restart"
p41="$p02/besu.restart"
p42="$p02/beacon.restart"
p43='/sys/devices/system/cpu/intel_pstate/no_turbo'
p44='/sys/bus/usb/devices/1-0:1.0/authorized'
p45='/var/run/reboot-required'
p46="$p02/validator.restart"

r00='s%^ *#*NTP=.*$%NTP=192.168.8.28 ntp02.oal.ul.pt ntp04.oal.ul.pt time.google.com time.cloudflare.com time.facebook.com%i'
r01='s%^ *ENABLED=.*$%ENABLED=0%i'
r02='s%bind[^"]*"%bind -4"%'
r03='s%^ */*\(.*codename}-updates.*$\)%\1%i'
r03="${r03};s%^ */* *Unattended-Upgrade::Mail .*$%Unattended-Upgrade::Mail \"hernanilr@gmail.com\";%i"
