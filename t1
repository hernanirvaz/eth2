e53="s%^ *$p01%# $p01%"
e54='s%.*synchronized: *([a-z]*) *%\1%p'
e55='s%[0-9]*:[0-9]*:[0-9]*%%;s%[0-9]* users*,%%;s% average([^,]*).*%\1%;s%  *% %g'
e56='s%.*result" *: *"0x([^"]+)".*%\1%ip'
e57='s%.*Active: *(.*)%\1%p'
e58='s% *since.*;%%'
