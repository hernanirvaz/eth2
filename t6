  c1="$c1\ndsh [url]\tdns show"
  c1="$c1\nnrs\t\tdns/netplan reset"
  c1="$c1\ndrs\t\tdns reset"
  c1="$c1\ntrs\t\ttime sync restart"
  echo -e "\n$c1\n"
fi
