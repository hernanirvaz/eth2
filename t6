  c1="$c1\np\t\t$ethp"
  c1="$c1\nn\t\t$ethn"
  c1="$c1\nf\t\t$ethf"
  c1="$c1\nx\t\t$plex"
  c1="$c1\nr\t\t$trrt"
  c1="$c1\ns\t\t$dnsd"
  c1="$c1\n"
  c1="$c1\nsegunda letra"
  c1="$c1\na\t\tarranca"
  c1="$c1\naj\t\tarranca\t& journal"
  c1="$c1\ne\t\tenable"
  c1="$c1\nj\t\tjournal"
  c1="$c1\npr\t\tpara"
  c1="$c1\nds\t\tdisable"
  c1="$c1\npd\t\tpara\t& disable"
  c1="$c1\npa\t\tpara\t& arranca\t& journal"
  c1="$c1\nea\t\tenable\t& arranca\t& journal"
